{
  "name": "chat_arayuzu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:ci": "vitest run && cypress run",
    "test:all": "npm run lint && npm run test && npm run test:e2e",
    "test:static": "tsc --noEmit && eslint .",
    "test:unit": "vitest run",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:docker": "powershell -File ./scripts/run-tests-docker.ps1",
    "test:docker:watch": "powershell -File ./scripts/run-tests-docker.ps1 -Watch",
    "test:docker:e2e": "powershell -File ./scripts/run-tests-docker.ps1 -TestType e2e",
    "test:docker:static": "powershell -File ./scripts/run-tests-docker.ps1 -TestType static",
    "test:k8s": "powershell -File ./scripts/run-tests-k8s.ps1",
    "test:k8s:scheduled": "powershell -File ./scripts/run-tests-k8s.ps1 -Scheduled",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:a11y": "cypress run --config-file cypress.a11y.config.ts",
    "test:visual": "cypress run --config-file cypress.visual.config.ts",
    "typecheck": "tsc --noEmit",
    "generate-test-report": "node scripts/generate-test-report.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.9.0",
    "framer-motion": "^10.16.16",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.0.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.6.6",
    "eslint": "^9.27.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^6.5.0",
    "eslint-plugin-vitest": "^0.3.26",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
